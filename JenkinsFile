pipeline {
    agent any

    stages {
        stage('Read File From Workspace') {
            steps {
                sh 'pwd'
                sh 'ls -log'
            }
        }
        stage('check if File Exist'){
            steps{
                script{
                    def workingDirectory = pwd();
                    def directoryName = workingDirectory + '/src/main/resources';
                    def op = sh(script: "ls /directoryName -log",returnStdout: true).trim()
                    print(op)
                }
            }
        }
    }
}
